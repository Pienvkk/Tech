<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>F1 Matching</title>
    <link rel="stylesheet" href="/style/style.css" type="text/css">
    <link rel="stylesheet" href="https://use.typekit.net/jva5hqf.css">
</head>

<body>
    <%- include('../view/header'); %>

    <main class="mainCommunity">
        <h1>Community</h1>
        <div>
            <% posts.forEach(post => { %> <!--hoe maak ik deze posts <-< -->
                <article>
                    <h2><%= post.title %></h2>
                    <p><%= post.content %></p>
                    <img src="" alt="hier zou een foto komen">
                </article>
                
            <% }) %>
        </div>
        <a href="/createPost">Create Post</a>
    </main>

    <%- include('../view/footer'); %>
    <script src="/script.js"> </script>

</body>
</html>